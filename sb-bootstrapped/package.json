{
  "name": "sb-simple-setup",
  "version": "1.0.0",
  "description": "Simple setup project for React, Webpack, Typesript",
  "scripts": {
    "init": "npx storybook@latest init --package-manager npm",
    "init-webpack": "npx storybook@latest init --package-manager npm --type react --builder webpack5",
    "clear-cache": "rimraf node_modules/.cache/storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "m.kubelik@protank.cz",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/react-webpack5": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^17.0.65",
    "@types/react-dom": "^17.0.20",
    "storybook": "^7.4.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
